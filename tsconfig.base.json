{
  "compilerOptions": {
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "strictFunctionTypes": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "suppressImplicitAnyIndexErrors": true,
    "moduleResolution": "node",
    "stripInternal": false,
    "target": "es5",
    "outDir": "./out",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es5", "dom"]
  }
}
